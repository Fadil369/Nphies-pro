{
  "name": "nphies-pro-digital-insurance",
  "version": "1.0.0",
  "description": "BrainSAIT Digital Insurance Platform - Comprehensive SaaS solution for Saudi Arabia's healthcare digitization",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "type-check": "turbo type-check",
    "setup:aws": "./scripts/setup-aws.sh",
    "deploy": "./scripts/deploy.sh",
    "dev:services": "concurrently \"npm run dev:tenant-manager\" \"npm run dev:claims-ai\"",
    "dev:tenant-manager": "cd services/tenant-manager && npm run dev",
    "dev:claims-ai": "cd services/claims-ai-engine && python src/main.py",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "insurance",
    "healthcare",
    "nphies",
    "fhir",
    "saas",
    "multi-tenant",
    "saudi-arabia",
    "brainsait"
  ],
  "author": "BrainSAIT Digital Solutions",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "devDependencies": {
    "turbo": "^1.12.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "concurrently": "^8.2.2"
  }
}